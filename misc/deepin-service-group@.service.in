[Unit]
Description=deepin-service-manager
After=dbus.service deepin-service-manager.service
PartOf=deepin-service-manager.service

[Service]
ExecStart=/usr/bin/deepin-service-manager -g %i
Restart=always
RestartSec=3
@SYSTEMD_SLICE@

@SYSTEMD_INSTALL@

ProtectSystem=strict
InaccessiblePaths=/etc/shadow
ReadWritePaths=-/usr/share/deepin-service-manager/
ReadWritePaths=-/usr/local/lib/deepin-service-manager/
ReadWritePaths=-/usr/local/deepin-service-manager/
